{
  "name": "graphql-apollo-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.ts",
  "exports": "./dist/main.js",
  "scripts": {
    "postbuild": "cpy '**/*' '!**/*.ts' '../dist/' --cwd=src --parents",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "watch": "nodemon --watch './**/*.{ts,graphql}' --exec \"node --experimental-specifier-resolution=node --loader ts-node/esm\" src/main.ts",
    "dev": "tsc-watch --onSuccess \"yarn watch\"",
    "prestart:dev": "yarn postbuild",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:local": "NODE_ENV=local yarn start",
    "prestart:prod": "rimraf dist && yarn build",
    "start:prod": "node dist/main.js",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "knex": "node -r dotenv/config -r ts-node/register --unhandled-rejections=strict node_modules/knex/bin/cli",
    "migrate:make": "yarn knex migrate:make",
    "migrate:up": "yarn knex migrate:latest",
    "migrate:down": "yarn knex migrate:down",
    "migrate:down:all": "yarn knex migrate:rollback --all",
    "seed:create": "yarn knex seed:make",
    "seed": "yarn knex seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.72.0",
    "@faker-js/faker": "^6.2.0",
    "@grpc/grpc-js": "^1.6.7",
    "@types/uuid": "^8.3.4",
    "apollo-datasource-rest": "^3.5.2",
    "apollo-server-core": "^3.6.7",
    "apollo-server-express": "^3.6.7",
    "class-validator": "^0.13.2",
    "concurrently": "^7.1.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "google-protobuf": "^3.20.1",
    "graphql": "^15.3.0",
    "grpc": "^1.24.11",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.7",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.7.0",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/google-protobuf": "^3.15.5",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "cpy-cli": "^4.1.0",
    "eslint": "^8.13.0",
    "grpc-tools": "^1.11.2",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "2.6.2",
    "protoc-gen-ts": "^0.8.2",
    "ts-jest": "^27.1.4",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  }
}
